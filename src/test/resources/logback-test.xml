<configuration scan="false" debug="false">
  <appender name="CONSOLE" class="com.syakeapps.jtp.testutil.NOPAppender">
    <encoder>
      <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%logger] \(%thread\) %msg%n</pattern>
    </encoder>
  </appender>
  <root level="ALL">
    <appender-ref ref="CONSOLE" />
  </root>
</configuration>